{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
  "theme": "tribe",
  "active": "default",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      {
        "name": "hello",
        "autoExecuteCommands": true,
        "icon": "person",
        "color": "terminal.ansiGreen",
        "commands": ["echo hello"]
      },
      [
        {
          "name": "idle-service",
          "shell": "powershell",
          "autoExecuteCommands": true,
          "commands": ["cd idle-service; npm start"]
        },
        {
          "name": "auth-service",
          "shell": "powershell",
          "autoExecuteCommands": true,
          "commands": ["cd auth-service; npm start"]
        }
      ],
      [
        {
          "name": "factory-service",
          "shell": "powershell",
          "autoExecuteCommands": true,
          "commands": ["cd factory-service; npm start"]
        },
        {
          "name": "inventory-service",
          "shell": "powershell",
          "autoExecuteCommands": true,
          "commands": ["cd inventory-service; npm start"]
        }
      ]
    ],
    "saved-session": [
      {
        "name": "connect",
        "commands": [""]
      }
    ]
  }
}
